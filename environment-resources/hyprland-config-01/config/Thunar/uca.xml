<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>utilities-terminal</icon>
	<name>Open Terminal Here</name>
	<submenu></submenu>
	<unique-id>1756039040222978-1</unique-id>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Open terminal in containing directory</description>
	<range></range>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>kdenlive-show-video</icon>
	<name>Merge Video with Audio (ffmpeg)</name>
	<submenu></submenu>
	<unique-id>1756920693343957-1</unique-id>
	<command>/home/$USER/.config/Thunar/scripts/thunar-merge-audio-video.sh %F</command>
	<description>Replace or add audio track to a video file using ffmpeg</description>
	<range>2-2</range>
	<patterns>*.mp4;*.mkv;*.mov;*.avi;*.mp3;*.aac;*.wav;*.flac</patterns>
	<audio-files/>
	<video-files/>
</action>
<action>
	<icon>application-pdf</icon>
	<name>Create PDF</name>
	<submenu></submenu>
	<unique-id>1758744792026531-1</unique-id>
	<command>magick %F document.pdf</command>
	<description>Creating a PDF document from images</description>
	<range>*</range>
	<patterns>*.jpg;*.jpeg;*.png;*.svg</patterns>
	<image-files/>
	<other-files/>
</action>
</actions>
